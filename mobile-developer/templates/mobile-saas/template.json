{
  "name": "mobile-saas",
  "description": "Full mobile SaaS app with auth, payments, and push notifications",
  "stack": {
    "framework": "Expo SDK 51+",
    "auth": "Supabase Auth",
    "payments": "RevenueCat",
    "push": "Expo Notifications",
    "backend": "Supabase"
  },
  "dependencies": {
    "expo": "~51.0.0",
    "expo-router": "~3.5.0",
    "@supabase/supabase-js": "^2.0.0",
    "react-native-purchases": "^7.0.0",
    "expo-notifications": "~0.28.0",
    "react-native-mmkv": "^2.0.0"
  },
  "structure": [
    "app/",
    "app/(auth)/",
    "app/(main)/",
    "components/",
    "hooks/",
    "services/",
    "services/api.ts",
    "services/auth.ts",
    "services/purchases.ts",
    "store/",
    "types/"
  ],
  "features": {
    "auth": "Email, Google, Apple Sign-In",
    "payments": "Subscriptions via RevenueCat",
    "push": "Push notifications with deep linking",
    "offline": "MMKV for local storage",
    "sync": "Real-time sync with Supabase"
  },
  "env": [
    "EXPO_PUBLIC_SUPABASE_URL",
    "EXPO_PUBLIC_SUPABASE_ANON_KEY",
    "REVENUECAT_API_KEY"
  ]
}
