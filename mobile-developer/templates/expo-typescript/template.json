{
  "name": "expo-typescript",
  "description": "Expo app with TypeScript, navigation, and essential setup",
  "stack": {
    "framework": "Expo SDK 51+",
    "language": "TypeScript",
    "navigation": "Expo Router",
    "styling": "NativeWind (Tailwind)"
  },
  "dependencies": {
    "expo": "~51.0.0",
    "expo-router": "~3.5.0",
    "react-native": "0.74.0",
    "nativewind": "^4.0.0",
    "zustand": "^4.0.0"
  },
  "structure": [
    "app/",
    "app/_layout.tsx",
    "app/index.tsx",
    "app/(tabs)/",
    "components/",
    "hooks/",
    "services/",
    "store/",
    "types/",
    "app.json",
    "eas.json"
  ],
  "features": {
    "navigation": "File-based routing with Expo Router",
    "styling": "Tailwind CSS via NativeWind",
    "state": "Zustand for global state"
  }
}
